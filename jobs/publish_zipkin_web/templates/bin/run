#!/bin/bash

set -e # exit immediately if a simple command exits with a non-zero status
set -u # report the usage of uninitialized variables

# Setup env vars and folders for the webapp_ctl script
source /var/vcap/jobs/publish_zipkin_web/helpers/ctl_setup.sh 'publish_zipkin_web'

/var/vcap/jobs/publish_zipkin_web/bin/errand.sh

EXITSTATUS=0

echo "Errand publish_zipkin_web is complete; exit status $EXITSTATUS"
exit $EXITSTATUS
